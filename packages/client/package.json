  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/cheerio": "^0.22.35",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/eslint-plugin": "^1.1.39",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cheerio": "^1.1.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
