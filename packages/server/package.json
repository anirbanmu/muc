{
  "name": "@muc/server",
  "version": "1.0.0",
  "description": "Express backend for MUC project.",
  "type": "module",
  "main": "dist/server.bundle.cjs",
  "scripts": {
    "build": "esbuild src/server.ts --bundle --platform=node --target=node20 --outfile=dist/server.bundle.cjs --format=cjs --banner:js='#!/usr/bin/env node'",
    "start": "node --env-file=../../.env dist/server.bundle.cjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write . --ignore-path ../../.prettierignore",
    "format:check": "prettier --check . --ignore-path ../../.prettierignore"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@muc/common": "*",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "p-limit": "^7.1.1",
    "supports-color": "^10.2.2"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/morgan": "^1.9.10",
    "@types/supports-color": "^8.1.3"
  }
}
